<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rvcoder.scheduleclassifier"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
          <activity
            android:name="com.rvcoder.scheduleclassifier.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.rvcoder.scheduleclassifier.Schedule"
            android:label="@string/title_activity_schedule" >
        </activity>
        <activity
            android:name="com.rvcoder.scheduleclassifier.TransactionActivity"
            android:label="@string/title_activity_transaction" >
        </activity>
        <activity android:name="com.rvcoder.scheduleclassifier.FrontActivity" 
            android:label="Schedule Classification">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
                </intent-filter>
                
                </activity>
                <activity android:name="com.rvcoder.scheduleclassifier.Tutorial" 
                    android:label="Tutorial">
        </activity>
    </application>

</manifest>
